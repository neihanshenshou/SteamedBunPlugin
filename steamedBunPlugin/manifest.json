{
  "manifest_version": 3,
  "name": "虾仁",
  "version": "1.0.2",
  "description": "浏览器Cookie展示插件（虾仁版）",
  "permissions": [
    "cookies",
    "tabs",
    "activeTab",
    "webRequest",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png"
    }
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  }
}