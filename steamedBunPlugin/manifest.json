{
  "manifest_version": 3,
  "name": "虾仁",
  "version": "2.0.0",
  "description": "浏览器Cookie展示插件（虾仁版）",
  "permissions": [
    "cookies",
    "tabs",
    "activeTab",
    "webRequest",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "img.png"
    }
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  }
}